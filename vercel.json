{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/initialize",
      "dest": "index.js",
      "methods": ["POST"]
    },
    {
      "src": "/sheet",
      "dest": "index.js",
      "methods": ["POST"]
    },
    {
      "src": "/submit-sheet",
      "dest": "index.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
